{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "ApiMu",
  "patterns": [
    {
      "name": "keyword.control.apimu",
      "match": "\\b(any|anypred|anyfunc|assume|def|idef|undef|import|任取|任取谓词|任取函数|假设|定义|选取|删除定义|导入)\\b"
    },
    {
      "name": "keyword.control.apimu",
      "match": "=>|:=|::|:<->"
    },
    {
      "name": "keyword.other.apimu",
      "match": "\\b(name|by|since|proof|namespace|private|public|notation|prefix|suffix|infix|transitive|记为|根据|命名空间|不公开|公开|记号|前缀记号|后缀记号|中缀记号)\\b"
    },
    {
      "name": "constant.numeric.apimu",
      "match": "\\b(and\\.i|and\\.l|and\\.r|or\\.l|or\\.r|or\\.e|implies\\.i|implies\\.e|not\\.i|not\\.e|iff\\.i|iff\\.e|iff\\.l|iff\\.r|eq\\.i|eq\\.e|eq\\.symm|eq\\.trans|forall\\.i|forall\\.e|exists\\.i|exists\\.e|unique\\.i|unique\\.l|unique\\.r|forallpred\\.i|forallpred\\.e|forallfunc\\.i|forallfunc\\.e|true\\.i|false\\.e|raa|element)\\b"
    },
    {
      "name": "constant.numeric.apimu",
      "begin": "\\b[0-9]",
      "end": "\\b"
    },
    {
      "name": "entity.name.type.apimu",
      "match": "\\b(and|or|implies|not|iff|eq|true|false|forall|exists|unique|forallpred|forallfunc|且|或|意味着|并非|当且仅当|等于|真|假|对所有|存在着|唯一存在着|对所有谓词|对所有函数)\\b"
    },
    {
      "name": "entity.name.type.apimu",
      "match": "∧|∨|→|¬|↔|∃!|∀#|∀\\$|∀|∃|⊤|⊥|=|≠|∈|∉|⊆|->|<->|↑|\\b(in|notin|subsetof)\\b|\\|"
    },
    {
      "name": "variable.parameter.apimu",
      "match": "\\b_.*?\\b"
    },
    {
      "name": "string.quoted.double.apimu",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.apimu",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "comment.line.double-slash.apimu",
      "match": "//[^\\r\\n]*"
    },
    {
      "name": "comment.block.apimu",
      "begin": "/\\*",
      "end": "\\*/"
    },
    {
      "name": "meta.preprocessor.apimu",
      "match": "#[^ /]*"
    }
  ],
  "scopeName": "source.apimu"
}